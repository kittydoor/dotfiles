  Hostname gitlab.com
  User git
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_ed25519_aigent

Host kittydoor.gitlab.com
  Hostname gitlab.com
  User git
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_ed25519_master
